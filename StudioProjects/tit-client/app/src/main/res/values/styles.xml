<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.




        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

        <style name="titleBarText">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/textColorPrimary</item>
        <item name="android:gravity">center_vertical</item>
    </style>
   <style name="title_right_button">
        <item name="android:paddingLeft">@dimen/title_rightButton_padding</item>
        <item name="android:paddingRight">@dimen/title_rightButton_padding</item>
        <item name="android:clickable">true</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@drawable/back_button_default_style</item>
    </style>
    
       <style name="settings_item_title_tv">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/blue_text</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">17.0dip</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        
    </style>
    
        <style name="settings_item_content_tv_has_detail">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">17.0dip</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    
         <style name="settings_item_content_desc_tv">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:gravity">left</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">17.0dip</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="settings_item_content_tv">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">17.0dip</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="settings_item_option_tv">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/blue_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">17.0dip</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="settings_whitelist_count">
        <item name="android:textSize">18.0sp</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">50.0dip</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
        <style name="settings_right_arrow">
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">15.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/arrow</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    
    <style name="LoginEditText">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">@color/dark_grey</item>
        <item name="android:textColorHint">@color/light_grey</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">@color/transparent_color</item>
        <item name="android:paddingLeft">18.0dip</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingRight">18.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="HeadTxtWithShadow">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#b9000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
            <style name="full_image_progress_text">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffeeeeee</item>
    </style>
    <style name="full_image_progressbar" parent="@android:style/Widget.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@drawable/progress_large_holo</item>
    </style>
    <style name="full_image_retry_text">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffeeeeee</item>
    </style>
    
         <style name="SwipeBackLayout">
        <item name="edge_size">50dip</item>
        <item name="shadow_left">@drawable/shadow_left</item>
        <item name="shadow_right">@drawable/shadow_right</item>
        <item name="shadow_bottom">@drawable/shadow_bottom</item>
    </style>
      <style name="base_large_progressbar_style">
        <item name="android:maxWidth">38.0dip</item>
        <item name="android:maxHeight">38.0dip</item>
        <item name="android:indeterminateDrawable">@anim/base_loading_large_anim</item>
        <item name="android:minWidth">38.0dip</item>
        <item name="android:minHeight">38.0dip</item>
    </style>
     <style name="base_listview_style" parent="@android:style/Widget.ListView">
        <item name="android:scrollbarThumbVertical">@drawable/base_list_scrollbar_handle</item>
        <item name="android:fadingEdgeLength">0.0dip</item>
        <item name="android:alwaysDrawnWithCache">true</item>
        <item name="android:listSelector">@drawable/base_list_selector</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0.0dip</item>
    </style>
    
      <style name="Select_City_Theme" parent="@style/AppTheme">
        <item name="android:windowIsTranslucent">true</item>
    </style>
      <style name="TextAppearance.List.Primary" parent="@style/TextAppearance.List">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:shadowColor">@color/primary_text_color_shadow</item>
    </style>
      <style name="TextAppearance.List" parent="@style/TextAppearance" />
        <style name="TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
     <style name="Widget.Item.ListView.SingleLine" parent="@style/Widget.Item.ListView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/list_item_height_single_line</item>
    </style>
    <style name="Widget.Item.ListView" parent="@style/Widget.Item" />
        <style name="Widget.Item" parent="@style/Widget">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
      <style name="Widget" />
      <style name="TextAppearance.Small.Inverse" parent="@style/TextAppearance.Small">
        <item name="android:textColor">@color/primary_text_color_inverse</item>
    </style>
    <style name="TextAppearance.Small" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/text_size_small</item>
    </style>
     <style name="Widget.EditText" parent="@android:style/Widget.EditText">
        <item name="android:textAppearance">@style/TextAppearance.EditText</item>
        <item name="android:textColorHint">#ffcacccd</item>
        <item name="android:background">@drawable/input_bg</item>
        <item name="android:padding">@dimen/input_padding</item>
    </style>
     <style name="Widget.HyperLink" parent="@style/Widget">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textColor">@color/highlight_text_color</item>
        <item name="android:textColorHighlight">@color/highlight_text_color</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:background">@drawable/hyperlink_bg</item>
    </style>
    
       <style name="Widget.Button.Main" parent="@style/Widget.Button">
        <item name="android:textColor">@color/highlight_text_color</item>
        <item name="android:background">@drawable/btn_primary_bg</item>
        <item name="android:shadowColor">@android:color/black</item>
    </style>
        <style name="Widget.Button" parent="@style/Widget">
        <item name="android:textAppearance">@style/TextAppearance.Button</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_secondary_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/button_height</item>
    </style>
       <style name="TextAppearance.EditText" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@color/primary_text_color</item>
    </style>
        <style name="TextAppearance.Button" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:height">@dimen/button_height</item>
    </style>
     <style name="Widget.AutoCompleteTextView" parent="@style/Widget.EditText">
        <item name="android:popupBackground">@drawable/list_item_middle_bg_n</item>
        <item name="android:dropDownHorizontalOffset">0.0dip</item>
        <item name="android:dropDownVerticalOffset">0.0dip</item>
    </style>
</resources>